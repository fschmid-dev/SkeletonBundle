{
    "name": "fschmid-dev/skeleton-bundle",
    "description": "This is a symfony skeleton bundle",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "FSchmid",
            "email": "info@fschmid.dev"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "symfony/framework-bundle": "^4.4",
        "symfony/yaml": "^4.0|^5.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "symfony/phpunit-bridge": "^5.2",
        "friendsofphp/php-cs-fixer": "^2.18",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/maker-bundle": "^1.30"
    },
    "autoload": {
        "psr-4": {
            "FSchmid\\SkeletonBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FSchmid\\SkeletonBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
